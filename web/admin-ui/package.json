{
  "name": "badge-admin-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test:e2e": "playwright test --config=e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --ui --config=e2e/playwright.config.ts",
    "test:e2e:headed": "playwright test --headed --config=e2e/playwright.config.ts",
    "dev:real": "VITE_DISABLE_MOCK=true vite",
    "test:integration": "VITE_DISABLE_MOCK=true playwright test --config=e2e/playwright.config.ts --project=chromium e2e/specs/integration.spec.ts e2e/specs/api-integration.spec.ts e2e/specs/ui-integration.spec.ts",
    "test:integration:headed": "VITE_DISABLE_MOCK=true playwright test --headed --config=e2e/playwright.config.ts --project=chromium e2e/specs/integration.spec.ts e2e/specs/api-integration.spec.ts e2e/specs/ui-integration.spec.ts",
    "test:integration:all": "VITE_DISABLE_MOCK=true playwright test --config=e2e/playwright.config.ts --project=chromium",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.0",
    "@ant-design/pro-components": "^2.8.0",
    "@tanstack/react-query": "^5.62.0",
    "@xyflow/react": "^12.4.0",
    "ahooks": "^3.8.0",
    "antd": "^5.23.0",
    "axios": "^1.7.0",
    "dayjs": "^1.11.0",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.0",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.58.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "globals": "^15.14.0",
    "jsdom": "^28.0.0",
    "typescript": "~5.7.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  }
}
